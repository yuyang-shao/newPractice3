    </servlet-mapping>
    <servlet>
        <servlet-name>modifyServlet2</servlet-name>
        <servlet-class>syy.Servlet.modifyServlet2</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>modifyServlet2</servlet-name>
        <url-pattern>/modifyServlet2</url-pattern>
    </servlet-mapping>
</web-app>